<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainPanel" prefHeight="400.0" prefWidth="369.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainPageController">
   <children>
      <AnchorPane layoutX="6.0" layoutY="5.0" prefHeight="390.0" prefWidth="358.0" style="-fx-background-color: linear-gradient(to bottom right, #4c9f50,#e6ee9c);">
         <children>
            <ImageView fitHeight="323.0" fitWidth="345.0" layoutX="7.0" layoutY="53.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@img/paddy-rice-seed-woo-bowl-white_253984-1164.jpg" />
               </image>
            </ImageView>
            <Button fx:id="pricedetailsBtn" layoutX="64.0" layoutY="83.0" mnemonicParsing="false" onAction="#price" prefHeight="42.0" prefWidth="230.0" style="-fx-background-color: #827717; -fx-background-radius: 25%;" text="Paddy Price Details" textFill="#eae8d6">
               <font>
                  <Font name="Bodoni MT Bold" size="20.0" />
               </font>
            </Button>
            <Button fx:id="storagedetailsBtn" layoutX="42.0" layoutY="162.0" mnemonicParsing="false" onAction="#storage" prefHeight="50.0" prefWidth="274.0" style="-fx-background-color: #827717; -fx-background-radius: 25%;" text="Paddy Storage Details" textFill="WHITE">
               <font>
                  <Font name="Bodoni MT Bold" size="21.0" />
               </font>
            </Button>
            <Button fx:id="memberdetailsBtn" layoutX="75.0" layoutY="252.0" mnemonicParsing="false" onAction="#member" prefHeight="42.0" prefWidth="200.0" style="-fx-background-color: #827717; -fx-background-radius: 25%;" text="Member Details" textFill="#f9faef">
               <font>
                  <Font name="Bodoni MT Bold" size="22.0" />
               </font>
            </Button>
            <Label layoutX="124.0" layoutY="1.0" text="Main Menu" textFill="#004d40">
               <font>
                  <Font name="Bodoni MT Bold Italic" size="19.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
